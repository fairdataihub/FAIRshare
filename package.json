{
  "name": "soda-for-covid-19-research",
  "version": "0.0.1",
  "description": "Simplifying data curation for researchers working on COVID-19 research",
  "private": true,
  "keywords": [],
  "author": "Sanjay Soundarajan",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build-preclean": "node ./scripts/prebuild.js",
    "python:build": "python -m PyInstaller --onefile --clean ./src/pyflask/api.py --distpath ./src/pyflaskdist",
    "python:dev": "python ./src/pyflask/api.py",
    "electron:build": "vue-cli-service electron:build",
    "electron:build-release": "vue-cli-service electron:build -p always",
    "electron:serve-precopy": "copyfiles -u 1 ./src/pyflask/** ./dist_electron",
    "electron:serve": "yarn electron:serve-precopy && vue-cli-service electron:serve",
    "electron:dev:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fairdataihub/SODA-for-COVID-19-Research.git"
  },
  "dependencies": {
    "@electron/remote": "^1.2.1",
    "@element-plus/icons": "^0.0.11",
    "@tailwindcss/line-clamp": "^0.2.2",
    "axios": "^0.21.2",
    "axios-retry": "^3.2.0",
    "core-js": "^3.6.5",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "element-plus": "^1.1.0-beta.20",
    "fs-extra": "^10.0.0",
    "pinia": "^2.0.0-rc.10",
    "ps-tree": "^1.2.0",
    "semver": "^7.3.5",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-router": "4",
    "vue3-popper": "^1.4.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@iconify/vue": "^3.0.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "autoprefixer": "^9",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "electron": "^13.0.0",
    "electron-devtools-installer": "^3.1.0",
    "electron-notarize": "^1.1.1",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.19.1",
    "postcss": "^7",
    "prettier": "^2.3.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "tailwindcss-debug-screens": "^2.1.0",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
